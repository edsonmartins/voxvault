# VoxVault Python Orchestrator Configuration
# Copy this file to .env and fill in your values.

# Translation mode: disabled | claude | openai | local
VOXVAULT_TRANSLATION_MODE=disabled

# Target language for translation
VOXVAULT_TARGET_LANGUAGE=pt

# API Keys (required for cloud translation)
VOXVAULT_ANTHROPIC_API_KEY=
VOXVAULT_OPENAI_API_KEY=

# Rust WebSocket server URL
VOXVAULT_RUST_WS_URL=ws://localhost:8765

# Python API port
VOXVAULT_API_PORT=8766

# Session storage directory
VOXVAULT_SESSIONS_DIR=~/Documents/VoxVault/sessions

# SQLite database path
VOXVAULT_DB_PATH=~/Documents/VoxVault/voxvault.db
